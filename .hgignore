# use regexp syntax
syntax: regexp

# ignore automatically generated files
^target/
^papyrus/bin/
^papyrus/gen/
^papyrus/build/
^papyrus/target/
^test/bin/
^test/gen/
^test/build/
^test/target/

# ignore certain files
^test/proguard.cfg

# ingnore local build files
build\.keystore # specific to Papyrus
local\.properties # local android enviroment file

# ignore IDE project files
\.project
\.settings\/.*
\.classpath
\.metadata
\.checkstyle

# ignore temp files from various editors
\.DS_Store
.*~
.*\.swp
