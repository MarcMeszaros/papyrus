# use regexp syntax
syntax: regexp

# ignore automatically generated files
^bin/
^gen/
^build/
# ignore automatically generated files (JUnit testing)
^tests/bin/
^tests/gen/
^tests/build/
^tests/local\.properties

# ingnore local build files
^build\.keystore # specific to Papyrus
^local\.properties # local android enviroment file

# ignore IDE project files
\.project
\.settings\/.*
\.classpath

# ignore temp files
\.DS_Store
.*~
